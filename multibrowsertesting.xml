<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suitemultibrowser" parallel="tests">
  <test name="TestChrome">
  <parameter name="browsername" value="chrome"></parameter>
    <classes>
      <class name="MultiBrowserTesting.ParameterMultibrowser"/>
    </classes>
  </test> <!-- Testcompatibility -->
  <test name="TestFirefox">
  <parameter name="browsername" value="firefox"></parameter>
    <classes>
      <class name="MultiBrowserTesting.ParameterMultibrowser"/>
    </classes>
  </test>
  <test name="TestOpera">
  <parameter name="browsername" value="opera"></parameter>
    <classes>
      <class name="MultiBrowserTesting.ParameterMultibrowser"/>
    </classes>
  </test>
  <test name="TestEdge">
  <parameter name="browsername" value="edge"></parameter>
    <classes>
      <class name="MultiBrowserTesting.ParameterMultibrowser"/>
    </classes>
  </test> <!-- Testcompatibility -->
</suite> <!-- Suitemultibrowser -->
